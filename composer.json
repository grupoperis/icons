{
    "name": "grupoperis/icons",
    "description": "A custom icon collection for internal usage.",
    "keywords": ["Grupo Peris", "Icons"],
    "homepage": "https://github.com/grupoperis/icons",
    "license": "Copyright (c) 2023 PERISHOLD S.L.U.",
    "authors": [
        {
            "name": "Sergio Peris",
            "email": "speris@grupoperis.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-icons": "^1.4",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "GrupoPeris\\Icons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GrupoPeris\\Icons\\IconsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
